<settings>
	<!-- Socket -->
	<setting name="@mysql.socket" value="unix_socket=/var/run/mysqld/mysqld.sock" />
	
	<!-- MTA database -->
	<setting name="@mysql.database" value="MTA_DATABASE_NAME" />
	<setting name="@mysql.username" value="MTA_DATABASE_USERNAME" />
	<setting name="@mysql.password" value="MTA_DATABASE_PASSWORD" />
	<setting name="@mysql.hostname" value="MTA_DATABASE_HOST" />
	<setting name="@mysql.port" value="MTA_DATABASE_PORT" />
	
	<!-- Core database -->
	<setting name="@mysql.core_database" value="CORE_DATABASE_NAME" />
	<setting name="@mysql.core_username" value="CORE_DATABASE_USERNAME" />
	<setting name="@mysql.core_password" value="CORE_DATABASE_PASSWORD" />
	<setting name="@mysql.core_hostname" value="CORE_DATABASE_HOST" />
	<setting name="@mysql.core_port" value="CORE_DATABASE_PORT" />
	
	<!-- No need to fill in -->
	<setting name="imgurClient" value="IMGUR_API_KEY" />
	
	<!-- Inactivity Scanner -->
	<setting name="@payday.inactivityscanner_vehicles" value="1" />
	<setting name="@payday.inactivityscanner_interiors" value="1" />
	<setting name="@item-world.inactivityscanner_items" value="2" />
	<!--
	    Values:
		0: off, 
		1: delete all items after 30 days, 
		2: delete only exterior items after 30 days

		Notes (72) are deleted after 3 days.
		Storage items 81 (fridge), 103 (fridge), 223 (storage generic) and 231 (shipping container) are excempt from deletion
		Other excempt items: 169 (keyless digital door lock)
	-->
</settings>
